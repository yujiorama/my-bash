# shellcheck shell=bash

PATH=$(echo "$PATH" | tr ':' '\n' | grep -v -e '^$' | grep -v -E 'corretto|jdk|java' | tr '\n' ':')

export JAVA_OPTS
JAVA_OPTS="-Dfile.encoding=UTF-8"

export JAVA_TOOL_OPTIONS
JAVA_TOOL_OPTIONS="-Dfile.encoding=UTF-8"

if [[ "${OS}" = "Linux" ]]; then
    return
fi

export SCOOP_APP_JAVA_8=corretto8
export SCOOP_APP_JAVA_10=openjdk10
export SCOOP_APP_JAVA_11=graalvm-lts-jdk11
export SCOOP_APP_JAVA_16=liberica16-full-jdk
